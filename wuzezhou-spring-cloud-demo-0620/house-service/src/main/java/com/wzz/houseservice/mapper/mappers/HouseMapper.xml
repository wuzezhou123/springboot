        <result column="orientation" property="orientation" />
        <result column="decorate" property="decorate" />
        <result column="rent" property="rent" />
        <result column="pic" property="pic" />
        <result column="introduction" property="introduction" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, province, city, county, community, room, hall, toilet, orientation, decorate, rent, pic, introduction
    </sql>

</mapper>
